{
  "name": "your-vendor/avanak",
  "description": "A modern, well-tested, PSR-compliant PHP SDK for Avanak SMS API.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Farzaneh Labafi",
      "email": "Farzaneh.lbf@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1 || ^8.2 || ^8.3",
    "guzzlehttp/guzzle": "^7.9",
    "psr/log": "^3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Avanak\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Avanak\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
